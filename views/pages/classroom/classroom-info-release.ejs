<div id="classroom-info" v-cloak>
  <div class="container">
    <div class="container-fluid">
      <div class="row justify-content-md-center  m-3">
        <div class="col-sm">
          <h2>班级详情</h2>
        </div>
      </div>
    </div>
    <hr />
    <div class="container">
      <div class="bd-example-row">
        <div class="bd-example">
          <div class="container">
            <div v-for="(item, index) in classroominfoObj" :key="index" class="border-bottom p-3">
              <div class="row ">
                <div class="col-sm d-flex justify-content-between align-self-center">
                  <div class="d-flex flex-row bd-highlight mb-2">
                    <div class="mr-1 titletip align-self-center bg-info"></div>
                    <h4>
                      {{item.key}}
                    </h4>
                  </div>
                  <!-- <h5><span class="fa fa-minus-circle text-info" @click="reduceclick(item)"></span></h5> -->
                </div>
              </div>
              <div class="row">
                <div class="col-sm ml-1 text-muted">{{item.value}}</div>
              </div>
            </div>
          </div>
        </div>
      </div>
      
      <div class="container">
          <div class="border-bottom p-3">
            <div class="row ">
              <div class="col-sm d-flex justify-content-between align-self-center">
                <div class="d-flex flex-row bd-highlight mb-2">
                  <div class="mr-1 titletip align-self-center bg-info"></div>
                  <h4>
                    培训价格
                  </h4>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-sm ml-1 text-muted"> {{classroom.priceName}}</div>
            </div>
          </div>
          <div class="border-bottom p-3">
              <div class="row ">
                <div class="col-sm d-flex justify-content-between align-self-center">
                  <div class="d-flex flex-row bd-highlight mb-2">
                    <div class="mr-1 titletip align-self-center bg-info"></div>
                    <h4>
                      负责人
                    </h4>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-sm ml-1 text-muted"> {{classroom.personLiable}}</div>
              </div>
            </div>
            <div class="border-bottom p-3">
              <div class="row ">
                <div class="col-sm d-flex justify-content-between align-self-center">
                  <div class="d-flex flex-row bd-highlight mb-2">
                    <div class="mr-1 titletip align-self-center bg-info"></div>
                    <h4>
                      二维码
                    </h4>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-sm ml-1 text-muted"> {{classroom.personLiable}}</div>
              </div>
            </div>
        </div>
        
      </div>
<!-- 
      <ajax-form action="updateClassroominfo" :syncing.sync="syncing" :cloud-error.sync="cloudError"
          @submitted="submittedForm()" :handle-parsing="handleParsingForm">
          <div class="container-fluid">
              <div class="row ">
                <div class="col-sm d-flex justify-content-center mt-4">
                  <ajax-button type="submit" :syncing="syncing" class="btn btn-dark">发布</ajax-button>
                </div>
              </div>
            </div>
		 
       </ajax-form>
        -->

    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
      aria-hidden="true">
      <div class="modal-dialog mecontent" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">New message</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <form>
              <div class="form-group">
                <label for="recipient-name" class="col-form-label">Recipient:</label>
                <input type="text" class="form-control" v-model="newobjkey">
              </div>
              <div class="form-group">
                <label for="message-text" class="col-form-label">Message:</label>
                <textarea class="form-control" v-model="newobjvalue"></textarea>
              </div>
            </form>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            <button type="button" class="btn btn-primary" data-dismiss="modal" @click="addInfo">Send message</button>
          </div>
        </div>
      </div>
    </div>





  </div>


</div>



<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>